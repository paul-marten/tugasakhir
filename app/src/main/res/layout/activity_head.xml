<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_head"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="paul.develops.ta.Activity.Body.HeadActivity">

    <TextView
        android:text="Kepala"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textSize="30sp"
        android:background="@android:color/holo_blue_dark"
        android:alpha="1"
        android:textColor="@color/cardview_light_background"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:focusable="false" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/head"
        android:layout_marginTop="32dp"
        android:id="@+id/imageView4"
        android:visibility="visible"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="30dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginTop="11dp"
        android:id="@+id/imgMouth"
        android:layout_below="@+id/imgNose"
        android:layout_alignStart="@+id/textView10" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginEnd="22dp"
        android:id="@+id/imgNose"
        android:layout_alignTop="@+id/imgLeftEye"
        android:layout_toStartOf="@+id/imgRightEye" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginEnd="13dp"
        android:id="@+id/imgLeftEye"
        android:layout_alignTop="@+id/imgRightEye"
        android:layout_toStartOf="@+id/imgNose" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginBottom="106dp"
        android:id="@+id/imgRightEye"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignEnd="@+id/textView10" />

    <ImageView
        android:layout_width="27dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginStart="28dp"
        android:id="@+id/imgRightEar"
        android:layout_marginBottom="27dp"
        android:layout_alignBottom="@+id/imgMouth"
        android:layout_toEndOf="@+id/imgRightEye" />

    <ImageView
        android:layout_width="27dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/blank"
        android:layout_marginEnd="22dp"
        android:id="@+id/imgLeftEar"
        android:layout_alignTop="@+id/imgRightEar"
        android:layout_toStartOf="@+id/imgLeftEye" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imgHair"
        android:layout_marginBottom="29dp"
        android:layout_above="@+id/imgRightEar"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
